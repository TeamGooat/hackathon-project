{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    "$EMAIL",
    "$EMAIL_PASSWORD",
    "$JWT_SECRET",
    "$REDIS_URL",
    "$FURL",
    "$BURL"
  ]
}
